for(int i=0;i<secret.length();i++){
    if(secret.charAt(i) == guess.charAt(i)) num_bull++;
    else{
        if(nums[secret.charAt(i)-'0']++ < 0) num_cow++;
        if(nums[guess.charAt(i)-'0']--  > 0) num_cow++;
    }
}
return num_bull+"A"+num_cow+"B";

